<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomFontTextView">
        <attr name="typeface" />
    </declare-styleable>

    <attr name="typeface" format="enum">
        <enum name="lato_light" value="0" />
        <enum name="lato_bold" value="1" />
        <enum name="lato_semibold" value="2" />
        <enum name="lato_regular" value="3" />
    </attr>
    <declare-styleable name="RippleView">
        <attr name="rippleColor" format="color" />
        <attr name="alphaFactor" format="float" />
        <attr name="hover" format="boolean" />
    </declare-styleable>


    <declare-styleable name="ImageMap">
        <attr name="map" format="string"/>
        <attr name="fitImageToScreen" format="boolean"/>
        <attr name="scaleFromOriginal" format="boolean"/>
        <attr name="maxSizeFactor" format="float"/>
    </declare-styleable>
</resources>